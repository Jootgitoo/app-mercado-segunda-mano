<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FragmentDeporte">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tbDeporte"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="#2D3640"
        android:minHeight="?attr/actionBarSize"
        android:textAlignment="center"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:title="DEPORTE"
        app:titleTextAppearance="@style/TextAppearance.AppCompat.Body1"
        app:titleTextColor="#FFFFFF" />

    <ImageView
        android:id="@+id/imagenPantalon2"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoPantalon2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tbDeporte"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pantalon2" />

    <ImageView
        android:id="@+id/imagenMonitor1"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoMonitor1"
        app:layout_constraintEnd_toStartOf="@+id/imagenMonitor2"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/zapatillas1" />

    <ImageView
        android:id="@+id/imagenTorre2"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoTorre2"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/camiseta1" />

    <TextView
        android:id="@+id/textViewPrecioTorre1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="32dp"
        android:text="69.99€"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imagenPortatil1"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPortatil1"
        app:layout_constraintEnd_toStartOf="@+id/imagenPantalon2"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tbDeporte"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pantalon1" />

    <TextView
        android:id="@+id/txtViewPrecioPortatil1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="24dp"
        android:text="44.99€"
        app:layout_constraintBottom_toTopOf="@+id/imagenMonitor1"
        app:layout_constraintEnd_toStartOf="@+id/textViewPrecioPortatil2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imagenZapatillas3"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoTorre1"
        app:layout_constraintEnd_toStartOf="@+id/imagenZapatillas3"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/zapatillas3" />

    <TextView
        android:id="@+id/textViewPrecioMonitor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="4dp"
        android:text="135.00€"
        app:layout_constraintBottom_toTopOf="@+id/imagenTorre2"
        app:layout_constraintEnd_toStartOf="@+id/textViewPrecioZapatillas2"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewPrecioTorre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="28dp"
        android:text="69.99€"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtViewInfoMonitor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="8dp"
        android:text="Zapatillas Running"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewPrecioMonitor2"
        app:layout_constraintEnd_toStartOf="@+id/txtViewInfoZapatillas2"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewInfoTorre1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="4dp"
        android:text="Zapatillas Nike"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewPrecioTorre1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewPrecioPortatil2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="16dp"
        android:text="47.99€"
        app:layout_constraintBottom_toTopOf="@+id/imagenMonitor2"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imagenMonitor2"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoZapatillas2"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/zapatillas2" />

    <TextView
        android:id="@+id/txtViewInfoTorre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="8dp"
        android:text="Camiseta Running"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewPrecioTorre2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtViewInfoZapatillas2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="20dp"
        android:text="Zapatillas LiveStile"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/imagenTorre2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textViewPrecioZapatillas2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="16dp"
        android:text="69.99€"
        app:layout_constraintBottom_toTopOf="@+id/imagenTorre2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtViewInfoZapatillas2"
        app:layout_constraintVertical_bias="0.133" />

    <TextView
        android:id="@+id/txtViewInfoPantalon2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="228dp"
        android:layout_marginBottom="8dp"
        android:text="Pantalon Chandal Hombre"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPortatil1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPortatil1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="8dp"
        android:text="Pantalon Chandal Hombre"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPortatil1"
        app:layout_constraintStart_toStartOf="parent" />

</FrameLayout>