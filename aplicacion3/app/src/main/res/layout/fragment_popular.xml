        app:srcCompat="@drawable/sudadera1" />

    <TextView
        android:id="@+id/txtViewPopular1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="1dp"
        android:text="@string/sudaderaAzul"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPopular1"
        app:layout_constraintEnd_toStartOf="@+id/txtViewPrecioPopular1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioPopular1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="8dp"
