<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PantallaPrincipal">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tbModa"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@color/azulTbFragment"
        android:minHeight="?attr/actionBarSize"
        android:textAlignment="center"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:title="MODA"
        app:titleTextAppearance="@style/TextAppearance.AppCompat.Body1"
        app:titleTextColor="#FFFFFF" />

    <ImageView
        android:id="@+id/imagenSudadera1"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginTop="12dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoSudadera1"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tbDeporte"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/sudadera1" />

    <TextView
        android:id="@+id/txtViewInfoSudadera1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="1dp"
        android:text="Sudadera Azul"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioSudadera1"
        app:layout_constraintEnd_toStartOf="@+id/txtViewSudadera2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioSudadera1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="8dp"
        android:text="23.55€"
        app:layout_constraintBottom_toTopOf="@+id/imagenPantalon3"
        app:layout_constraintEnd_toStartOf="@+id/txtViewPrecioSudadera2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioPopular3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="67.56€"
        app:layout_constraintBottom_toTopOf="@+id/imagenPopular5"
        app:layout_constraintEnd_toStartOf="@+id/txtPrecioPopular4"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPopular3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="1dp"
        android:text="Pantalon Ancho"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPopular3"
        app:layout_constraintEnd_toStartOf="@+id/txtViewPopular4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imagenPopular5"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="99dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPopular5"
        app:layout_constraintEnd_toStartOf="@+id/imagenPopular6"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/jersey1" />

    <ImageView
        android:id="@+id/imagenPopular6"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewInfoPopular6"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/jersey2" />

    <TextView
        android:id="@+id/txtViewInfoPopular6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:text="JeseyMarron"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPopular6"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioPopular6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="40dp"
        android:text="40€"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtViewPopular5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="1dp"
        android:text="Jersey Verde"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioPopular5"
        app:layout_constraintEnd_toStartOf="@+id/txtViewInfoPopular6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioPopular5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="40dp"
        android:text="70€"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtViewPrecioPopular6"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewSudadera2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tbDeporte"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/sudadera2" />

    <TextView
        android:id="@+id/txtViewSudadera2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="1dp"
        android:text="Sudadera Rosa"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPrecioSudadera2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtViewPrecioSudadera2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="12dp"
        android:text="45.99€"
        app:layout_constraintBottom_toTopOf="@+id/imagenPopular3"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imagenPantalon3"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPopular3"
        app:layout_constraintEnd_toStartOf="@+id/imagenPopular3"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pantalon3" />

    <ImageView
        android:id="@+id/imagenPopular3"
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/txtViewPopular4"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/jersey3" />

    <TextView
        android:id="@+id/txtViewPopular4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="1dp"
        android:text="Jersey Punto"
        app:layout_constraintBottom_toTopOf="@+id/txtPrecioPopular4"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtPrecioPopular4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="12dp"
        android:text="89.00€"
        app:layout_constraintBottom_toTopOf="@+id/imagenPopular6"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>